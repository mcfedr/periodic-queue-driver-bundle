{
    "name": "mcfedr/periodic-queue-driver-bundle",
    "type": "symfony-bundle",
    "description": "A bundle for running symfony background tasks with doctrine as the driver",
    "keywords": ["queue", "job", "schedule", "task", "doctrine"],
    "autoload": {
        "psr-4": { "Mcfedr\\PeriodicQueueDriverBundle\\": "src/Mcfedr/PeriodicQueueDriverBundle/"}
    },
    "autoload-dev": {
        "psr-4": { "Mcfedr\\PeriodicQueueDriverBundle\\" : "tests/Mcfedr/PeriodicQueueDriverBundle/" },
        "classmap": ["tests/TestKernel.php"]
    },
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^3.0|^4.0",
        "mcfedr/queue-manager-bundle": "^5.6.1",
        "nesbot/carbon": "^1|^2",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "symfony/monolog-bundle": "^2.8|^3.0",
        "friendsofphp/php-cs-fixer": "^2.1",
        "symfony/browser-kit": "^3.0|^4.0",
        "symfony/yaml": "^3.4|4.1",
        "phpstan/phpstan": "^0.10.3"
    },
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "license": "MIT"
}
